                        xmlns:wadl="http://wadl.dev.java.net/2009/02" xml:lang="EN">
                        <para>The location of the host.
                        Must be a dictinoary with a single parameter 'locn'.</para>
                    </wadl:doc>
                </param>
            </representation>
            <representation mediaType="application/json">
